{
    "compilerOptions": {
        "allowJs": true,
        "removeComments": true,
        "target": "ES3",
        "outDir": "dist/",
        "alwaysStrict": true,
        "strict": true,
        "downlevelIteration": true,
        "incremental": true,
        "tsBuildInfoFile": ".cache/.tsbuildinfo"
    },
    "include": [
        "src",
    ],
    "exclude": [
        "dist",
    ],
}
