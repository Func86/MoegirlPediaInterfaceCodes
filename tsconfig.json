{
    "compilerOptions": {
        "allowJs": true,
        "removeComments": true,
        "target": "ES3",
        "outDir": "dist/",
        "alwaysStrict": true,
        "strict": true,
        "downlevelIteration": true,
        "incremental": true,
        "lib": [
            "ES5",
            "ES2015",
            "ES2016",
            "ES2017",
            "ES2018",
            "ES2019",
            "ES2020",
            "ES2021",
            "ES2022",
            "ESNext",
            "DOM"
        ],
        "ignoreDeprecations": "5.0"
    },
    "include": [
        "src",
    ],
    "exclude": [
        "dist",
    ],
}
